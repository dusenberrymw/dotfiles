[user]
	name = Mike Dusenberry
	email = EMAIL
[core]
	excludesfile = ~/.gitignore_global
	editor = vim
[credential]
	helper = osxkeychain
[push]
	default = simple
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
